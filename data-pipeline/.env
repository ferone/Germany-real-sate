

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=germany_real_estate

MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB=germany-real-estate

# Data Collection Settings
COLLECTION_INTERVAL=3600  # Time between collection runs in seconds
MAX_RETRIES=3  # Maximum number of retries for failed requests
REQUEST_TIMEOUT=30  # Request timeout in seconds
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=data_pipeline.log

# Rate Limiting
RATE_LIMIT_REQUESTS=10  # Number of requests
RATE_LIMIT_PERIOD=60  # Time period in seconds